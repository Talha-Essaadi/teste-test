#include "libft.h"

void	ft_lstiter(t_list *lst, void (*f)(void *))
{
	t_list	*acc;

	if (lst == NULL || f == NULL)
		return ;
	acc = lst;
	while (acc != NULL)
	{
		f(acc->content);
		acc = acc->next;
	}
}
