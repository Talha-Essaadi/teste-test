#include "libft.h"

void	ft_lstadd_back(t_list **lst, t_list *new)
{
	t_list	*acc;

	acc = *lst;
	while (acc->next != NULL)
		acc = acc->next;
	acc->next = new;
	new->next = NULL;
}
